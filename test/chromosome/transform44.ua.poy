(* Read unannotated chromosome using default parameter.  Locus breakpoint set to 1000 *)

set(seed:1,timer:0)
read(chromosome:("FILENAME1"))
transform(chromosome:(locus_indel:(10,0.9),locus_breakpoint:1000,annotate:(mauve,35.0,0.30,0.01,0.10),circular:false))
transform(fixed_states:("chr4", ignore_polymorphism))
build(2)
